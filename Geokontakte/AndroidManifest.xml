<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.geokontakte"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="15" />

    <application
        android:name="KontaktSpeicher"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Startmenu"
            android:label="@string/title_activity_startmenu" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Kontakte_anzeigen"
            android:label="@string/title_activity_kontakte_anzeigen" >
        </activity>
        <activity
            android:name=".Kontakte_anlegen"
            android:label="@string/title_activity_kontakte_anlegen" >
        </activity>
        <activity
            android:name=".Kontakt"
            android:label="@string/title_activity_kontakt" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>